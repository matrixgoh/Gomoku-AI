{
  "name": "gomoku-ai-server",
  "version": "1.0.0",
  "description": "Node.js server for Gomoku AI game",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "start": "node server.js",
    "start:esm": "node server.js",
    "start:simple": "node simple-server.cjs",
  "dev": "node --watch server.js",
    "build": "npm run build:client",
    "build:client": "cd ../client && npm run build && cp -r dist/* ../server/public/",
    "setup": "npm install && cd ../client && npm install",
  "serve": "node server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "@google/genai": "^1.27.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17"
  },
  "keywords": [
    "gomoku",
    "ai",
    "game",
    "gemini",
    "nodejs"
  ],
  "author": "Gomoku AI Team",
  "license": "MIT"
}